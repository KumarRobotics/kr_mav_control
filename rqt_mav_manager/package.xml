<package format="2">
  <name>rqt_mav_manager</name>
  <version>0.1.0</version>
  <description>Provides a GUI plugin for MAV Manager.</description>
  <maintainer email="ablasdel@gmail.com">Dinesh Thakur</maintainer>
  <maintainer email="kartikmohta@gmail.com">Kartik Mohta</maintainer>

  <license>BSD</license>

  <url type="repository">https://github.com/KumarRobotics/kr_ui</url>

  <author>Dinesh Thakur</author>

  <buildtool_depend>catkin</buildtool_depend>

  <exec_depend>rospy</exec_depend>
  <exec_depend>rqt_gui_py</exec_depend>
  <exec_depend>kr_mav_manager</exec_depend>
  <exec_depend>std_srvs</exec_depend>
  <exec_depend>rqt_gui</exec_depend>

  <export>
    <rqt_gui plugin="${prefix}/plugin.xml"/>
  </export>
</package>
