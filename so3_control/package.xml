<?xml version="1.0"?>
<package format="2">
  <name>so3_control</name>
  <version>2.0.0</version>
  <description>so3_control</description>
  <maintainer email="kartikmohta@gmail.com">Kartik Mohta</maintainer>
  <maintainer email="michael.f.watterson@gmail.com">Michael Watterson</maintainer>
  <maintainer email="justinthomas@jtwebs.net">Justin Thomas</maintainer>

  <license>BSD</license>

  <url type="website">http://ros.org/wiki/so3_control</url>
